				<maml:name>ServiceName</maml:name>
				<maml:description>
					<maml:para>Specifies the new or existing service name.</maml:para>
				</maml:description>
				<command:parameterValue required="true" variableLength="false">string</command:parameterValue>
			</command:parameter>
			<command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true (ByValue, ByPropertyName)" position="named">
				<maml:name>VMs</maml:name>
				<maml:description>
					<maml:para>A list of virtual machine objects to create.</maml:para>
				</maml:description>
				<command:parameterValue required="true" variableLength="true">PersistentVM[]</command:parameterValue>
			</command:parameter>
			<command:parameter required="false" variableLength="false" globbing="false" pipelineInput="true (ByPropertyName)" position="named">
				<maml:name>AffinityGroup</maml:name>
				<maml:description>
					<maml:para>Specifies the Windows Azure affinity group the cloud service will reside in. Only valid when creating a new cloud service.</maml:para>
